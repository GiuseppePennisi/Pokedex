{
    "name": "pokeball-loader-ui",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/pokeball-loader-ui/src",
    "prefix": "pk",
    "tags": [],
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "@nx/angular:package",
            "options": {
                "assets": [
                    {
                        "glob": "**/*",
                        "input": "libs/pokeball-loader-ui/src/lib/assets",
                        "output": "assets"
                    }
                ]
            }
        },
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "libs/pokeball-loader-ui/jest.config.ts"
            }
        },
        "lint": {
            "executor": "@nx/eslint:lint"
        }
    }
}
